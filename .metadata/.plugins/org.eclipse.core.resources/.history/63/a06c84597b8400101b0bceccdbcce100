package com.yedam;

import java.util.Scanner;

class Score{
	
	public void input() {
		Scanner scan = new Scanner(System.in);
		
		int n1 = scan.nextInt();
		int n2 = scan.nextInt();
		int n3 = total(n1, n2);
		
		System.out.println("result" + n3);
	}
	
	public int total(int n1, int n2) {
		int result = n1 + n2;
		return resultl
	}
	
}



